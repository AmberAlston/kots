{
  "consumer": {
    "name": "ship-cluster-ui"
  },
  "provider": {
    "name": "ship-cluster-api"
  },
  "interactions": [
    {
      "description": "a query to sign up for a new shipauth account",
      "request": {
        "method": "POST",
        "path": "/api/v1/signup",
        "headers": {
          "Content-Type": "application/json"
        },
        "body": {
          "email": "test-ship-auth-signup@gmail.com",
          "password": "password"
        }
      },
      "response": {
        "status": 200,
        "headers": {
          "Content-Type": "application/json"
        },
        "body": {
          "token": "generated",
          "signup": {
            "email": "test-ship-auth-signup@gmail.com",
            "id": "generated"
          }
        },
        "matchingRules": {
          "$.body.token": {
            "match": "type"
          },
          "$.body.signup.id": {
            "match": "type"
          }
        }
      }
    }
  ],
  "metadata": {
    "pactSpecification": {
      "version": "2.0.0"
    }
  }
}